<launch>
    <!-- turtlebot3 world open -->
    <include file="$(find turtlebot3_gazebo)/launch/turtlebot3_world.launch"></include>
    <!-- turtlebot3 navigation map file open -->  <!-- 만약에 map파일을 바꾸고 싶다면 아래의 map.yaml위치를 본인에게 맞게 수정하면 된다. -->
    <arg name="map_file" default="$HOME/catkin_ws/src/mode_control/map/map.yaml"/>
    <include file="$(find navigation_mode)/launch/turtlebot3_navigation.launch"></include>

    <!-- <node pkg="navigation_mode" name="turson_box_data" type="turson_box_data.py" output="screen"></node> -->
    <!-- <node pkg="navigation_mode" name="box2coordinate" type="box2coordinate.py" output="screen"></node>
    <node pkg="navigation_mode" name="move_turson" type="move_turson.py" output="screen"></node>
    <node pkg="navigation_mode" name="navigation_status" type="navigation_status.py" output="screen"></node> -->

    
</launch>
